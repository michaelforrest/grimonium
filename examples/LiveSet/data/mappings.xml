		<song name="Some Day" tempo="140" sceneoffset="43">
			<controls>
				<keyboard channel="3">piano</keyboard>
			</controls>
			<notes><text></text></notes>
			<group id="beats" track="1" colour="red">
				<clip pad="0" scene="0"/>
				<clip pad="1" scene="1"/>
				<clip pad="2" scene="2"/>
				<clip pad="3" scene="3"/>
				<clip pad="4" scene="4"/>
				<clip pad="5" scene="5"/>
			</group>
			<group id="bass" track="2" colour="green">
				<clip pad="6" scene="1"/>
				<clip pad="7" scene="2"/>
				<clip pad="8" scene="3"/>
				<clip pad="9" scene="4"/>
				<clip pad="10" scene="5"/>
			</group>
			<group id="music" track="3" colour="blue">
				<clip pad="11" scene="0"/>
				<clip pad="12" scene="1"/>
				<clip pad="13" scene="3"/>
				<clip pad="14" scene="4"/>
				<clip pad="15" scene="5"/>
			</group>
		</song>

		<song name="Bumpy Bosbo" colour="yellow" tempo="133" sceneoffset="0" pic="songs/cat.png">
			<notes>
				<text>This one goes around G and D
					Play bendy synth.</text>
			</notes>
			<controls>
				<keyboard>
					<range channel="2" transpose="24" low="0" high="62">BASS</range>
					<range channel="3" transpose="-12" low="63" high="120">STRINGS</range>
				</keyboard>
				<fader id="0" name="strings" channel="3" cc="71">
					<lcd text="strings"/>
				</fader>
				<fader id="2" name="bassfreq" channel="2" cc="71">
					<lcd text="bassfreq"/>
				</fader>
				<joystick camera="true">
					<pitchbend channel="2"/>
					<up name="mod wheel" channel="2" cc="1"/>
				</joystick>
			</controls>
			<!-- should validate for repetition -->
			<group id="beats" colour="red" track="0">
				<freqbone low="kick" med="snare" high="hihat"/>
				<clip pad="0" scene="0">
					<visual scene="0" />
				</clip>
				<clip pad="1" scene="1">
					<visual scene="1" />
				</clip>
				<clip pad="4" scene="2">
					<visual scene="2" />
				</clip>
				<clip pad="5" scene="3">
					<visual scene="3" />
				</clip>
				<clip pad="12" scene="4">
					<visual scene="4" />
				</clip>
			</group>
			<group id="notebones" track="9">
				<notebone note="C0" bone="bosbo"/>
			</group>
			<group id="rhythm" colour="yellow" track="2">
				<freqbone mid="bosbotrance"/>
				<clip pad="2" scene="0"/>
				<clip pad="6" scene="1"/>
			</group>
			<group id="lead" colour="pink" track="3">
				<freqbone high="bosbolead"/>
				<clip pad="3" scene="0"/>
				<clip pad="7" scene="1"/>
			</group>
			<group id="bass" colour="purple" track="1">
				<freqbone low="bosbobass"/>
				<clip pad="10" scene="0"/>
				<clip pad="11" scene="1"/>
			</group>
			<group id="bits" colour="green" track="3">
				<freqbone med="bosbobits" high="bosbohibits"/>
				<clip pad="13" scene="3"/>
			</group>
		</song>

		<song name="Song Of Technology" colour="green" sceneoffset="10">
			<notes>
				<text>Lorem ipsum dolor sit amet.</text>
			</notes>
			<controls>
				<keyboard channel="16" low="48" high="59">
					stretch/phaser
					loop/filter
					xloop/crush
					stutter/chorus
					tonal/scratchloop
					vinyl</keyboard>
				<encoder id="0" name="SOMETHING" channel="5" cc="15"/>
			</controls>
			<group id="notebones" track="9">
				<notebone note="C3" bone="ii"/>
				<notebone note="D3" bone="oo"/>
				<notebone note="E3" bone="squint"/>
				<notebone note="F3" bone="frown"/>
			</group>
			<group id="fxbones" track="16">
				<notebone note="48" bone="fx1"/>
				<notebone note="54" bone="fx2"/>
				<notebone note="51" bone="fx3"/>
				<notebone note="55" bone="fx4"/>
			</group>
			<group id="vocals" colour="pink" track="3">
				<clip pad="10" scene="1" >
					<visual scene="1" />
				</clip>
				<clip pad="11" scene="2" >
					<visual scene="2" />
				</clip>
				<clip pad="15" scene="3" >
					<visual scene="3" />
				</clip>
			</group>
			<group id="beats" colour="red" track="0">
				<clip pad="0" scene="0"/>
				<clip pad="4" scene="1"/>
				<clip pad="12" scene="3">
					<visual scene="0"/>
				</clip>
				<clip pad="7" scene="5">
					<visual scene="0"/>
				</clip>
			</group>
			<group id="bass" colour="purple" track="1">
				<clip pad="1" scene="0"/>
				<clip pad="5" scene="1"/>
				<clip pad="9" scene="2"/>
				<clip pad="13" scene="3"/>
				<clip pad="2" scene="4"/>
				<clip pad="6" scene="5"/>
			</group>
			<group id="trumpet" colour="yellow" track="2">
				<clip pad="14" scene="3">
					<visual scene="0"/>
				</clip>
			</group>


		</song>
		<song name="Violet Sky" sceneoffset="18" colour="purple">
			<notes>
				<image source="songs/violet_sky_score.png"/>
				<text/>
			</notes>
			<controls>
				<keyboard channel="3">piano</keyboard>
				<fader id="0" name="strings"  channel="3" cc="71">
					<lcd text="strings"/>
				</fader>
				<multilayerfader id="0">
					<range from="0" to="0.25" layer="strings1"/>
					<range from="0.25" to="0.5" layer="strings2"/>
					<range from="0.5" to="0.75" layer="strings3"/>
					<range from="0.75" to="1" layer="strings4"/>
				</multilayerfader>
				<!-- TODO: work out how to do overlapping ranges --> 
				<bonetempokeys tempo="0.15" low="40" high="F3" bone="cello"/>
				<bonetempokeys tempo="0.2" low="F#3" high="100" bone="viola"/>
				<noterangebone low="40" high="F3" bone="cellopitch"/> 
				<noterangebone low="F#3" high="100" bone="violapitch"/>
<!--				<bonetempokeys channel="3" tempo="0.15" low="A#1" high="F5" bone="cello"/>
				<bonetempokeys channel="3" tempo="0.2" low="F#5" high="D6" bone="viola"/>
				<noterangebone channel="3" low="A#1" high="D6" bone="cellopitch"/> 
				<noterangebone channel="3" low="A#1" high="D6" bone="violapitch"/>  -->
				
				<!-- piano...-->
				<bonetempokeyranges tempo="1" low="48" high="84" bone="piano" bonecount="23" channel="3"/>
		<!--		<bonetempokeyranges tempo="1" low="A#1" high="D6" bone="piano" bonecount="23" channel="3"/>-->
				
			</controls>
			<group id="beats" colour="red" track="0">
				<clip pad="0" scene="0"/>
				<clip pad="4" scene="1"/>
				<clip pad="8" scene="2"/>
			</group>
			<group id="bass" colour="blue" track="1">
				<clip pad="1" scene="0"/>
				<clip pad="5" scene="1"/>
			</group>
			<group id="trumpets" colour="yellow" track="2">
				<clip pad="6" scene="1"/>
				<clip pad="10" scene="2"/>
			</group>
		</song>


		<song name="Shift" tempo="100" colour="" sceneoffset="0">
			<controls>
				<keyboard channel="16" low="48" high="59">
					stretch/phaser
					loop/filter
					xloop/crush
					stutter/chorus
					tonal/scratchloop
					vinyl</keyboard>
					<fader id="0" name="Beats" max="127.0" track="0"/>
					<fader id="1" name="Bass"  max="127.0" track="1"/>
					<fader id="2" name="Music" max="127.0" track="2"/>
					<fader id="3" name="Music" max="127.0" track="3"/>
			</controls>
			<notes>
				<text />
			</notes>
			
		</song>
	</songs>

	<common>
		<encoder id="7" name="tempo" channel="1" cc="2">
			<lcd id="7" text="TEMPO"/>
		</encoder>
		<button id="SETTING" name="reverb" channel="2" cc="107" type="gate"/>
		<button id="MESSAGE" name="delay" channel="2" cc="106" type="toggle"/>
		<button id="SCENE" name="beatrepeat" channel="1" cc="10" type="gate">
			<lcd id="5" text="GRID"/>
			<lcd id="6" text="PITCH"/>
		</button>
		<encoder id="5" name="beatrepeat" channel="1" cc="4"/>
		<encoder id="6" name="pitch" channel="1" cc="5"/>
		<button id="EXIT" name="effectrix" channel="16" note="C3">
			<lcd id="6" text="PATTERN"/>
		</button>
		<fader id="7" name="FILTER" channel="3" cc="5">
			<lcd id="7" text="FILTER"/>
		</fader>
	
	</common>

