			</group>
			<group id="bass" colour="purple" track="5">
				<clip pad="12" scene="0"/>
			</group>
			<group id="bits" colour="green" track="6">
				<clip pad="13" scene="0"/>
			</group>
			<group id="bits" colour="green" track="7">
				<clip pad="14" scene="0"/>
			</group>
			<group id="lead" colour="pink" track="8">
				<noterangebone low="48" high="72" bone="horoskope"/>
				<clip pad="3" scene="0"/>
				<clip pad="7" scene="1"/>
				<clip pad="11" scene="2"/>
				<clip pad="15" scene="3"/>
