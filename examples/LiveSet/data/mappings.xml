<?xml version="1.0" encoding="UTF-8"?>
<set>
	<songs>
		<!-- BUMPY BOSBO -->
		<song name="Bumpy Bosbo" colour="yellow" tempo="133" sceneoffset="0">
			<stage zoom="0" pan="0">
				<layer id="bosbodrums"/>
				<layer id="basscat"/>
				<layer id="trancecat"/>
				<layer id="leadcat"/>
			</stage>
			<!-- should validate for repetition -->
			<group id="beats" colour="red" track="0">
				<freqbone low="kick" med="snare" high="hihat"/>
				<fader id="0" max="110.0"/>
				<clip pad="0" scene="0"/>
				<clip pad="1" scene="1"/>
				<clip pad="4" scene="2"/>
				<clip pad="5" scene="3"/>
				<clip pad="12" scene="4"/>
			</group>
			<group id="rhythm" colour="yellow" track="2">
				<freqbone mid="bosbotrance"/>
				<clip pad="2" scene="0"/>
				<clip pad="6" scene="1"/>
			</group>
			<group id="lead" colour="pink" track="3">
				<freqbone high="bosbolead"/>
				<clip pad="3" scene="0"/>
				<clip pad="7" scene="1"/>
			</group>
			<group id="bass" colour="purple" track="1">
				<freqbone low="bosbobass"/>
				<clip pad="10" scene="0"/>
				<clip pad="11" scene="1"/>
			</group>
			<group id="bits" colour="green" track="3">
				<freqbone med="bosbobits" high="bosbohibits"/>
				<clip pad="13" scene="3"/>
			</group>
		</song>
