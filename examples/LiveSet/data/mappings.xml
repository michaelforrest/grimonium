			</group>
			<group id="drums" track="4" colour="blue">
				<clip pad="13" scene="1"/>
			</group>
			<group id="bass" track="5" colour="purple">
				<clip pad="14" scene="1"/>
			</group>
			<group id="piano" track="6" colour="orange">
				<clip pad="15" scene="1"/>
			</group>
		</song>
		
		<song name="Horoskope" colour="red" tempo="105" sceneoffset="6">
			<notes>
				<text/>
			</notes>
			<group id="beats" colour="red" track="4" >
				<clip pad="0" scene="0"/>
			</group>
			<group id="bass" colour="purple" track="5">
