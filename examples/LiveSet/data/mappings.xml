<?xml version="1.0" encoding="UTF-8"?>
<set>
	<songs>
		<song name="Falling Leaves" tempo="120" sceneoffset="21" visuals="forest">
			<controls>
				<keyboard channel="3">piano</keyboard>
			</controls>
			<notes><text></text></notes>
			<group id="drone" track="0" colour="red">
				<clip scene="1"/>
			</group>
			<group id="electronic" track="1" colour="green">
				<clip scene="0"/>
				<clip scene="1"/>
				<clip  scene="2"/>
			</group>
			<group id="drums" track="4" colour="blue">
				<clip scene="1"/>
			</group>
			<group id="bass" track="5" colour="purple">
				<clip scene="1"/>
			</group>
			<group id="piano" track="6" colour="orange">
				<clip cene="1"/>
			</group>
		</song>
		
		<song name="Horoskope" colour="red" tempo="105" sceneoffset="6" visuals="robot">
			<controls>
				<keyboard channel="3">piano</keyboard>
				
				<fader id="0" name="strings" channel="3" cc="71">
					<lcd text="strings"/>
				</fader>
			</controls>
			<notes>
				<text>
					I know you like to think the world is doomed
					It's gonna blow up, blow up, quite soon
					What if I told you things are only getting better
					What if I told you that we never had it so good
					It's true, ooh ooh ooh
					It's true, ooh ooh ooh
					But nevertheless it isn't such a shame 
					There's not a problem that can be solved if you ignore it 
					And now it doesn't really matter what you do
					We're gonna make it anyway it's true, we always do
				</text>
			</notes>
			<group id="beats" colour="red" track="4" >
				<clip scene="0"/>
			</group>
			<group id="bass" colour="purple" track="5">
				<clip scene="0"/>
			</group>
			<group id="bits" colour="green" track="6">
				<clip scene="0"/>
			</group>
			<group id="bits" colour="green" track="7">
				<clip scene="0"/>
			</group>
			<group id="lead" colour="pink" track="8">
				<clip scene="0"/>
				<clip scene="1"/>
				<clip scene="2"/>
				<clip scene="3"/>
			</group>
			<group id="beats" colour="red" track="4" >
				<clip scene="1"/>
			</group>
		</song>

		<song name="Some Day" tempo="140" sceneoffset="43" visuals="bumpybosbo">
			<controls>
				<keyboard channel="3">piano</keyboard>
			</controls>
			<notes><text></text></notes>
			<group id="beats" track="0" colour="red">
				<clip scene="0"/>
				<clip scene="1"/>
				<clip scene="2"/>
				<clip scene="3"/>
				<clip scene="4"/>
				<clip scene="5"/>
			</group>
			<group id="bass" track="1" colour="green">
				<clip scene="1"/>
				<clip scene="3"/>
				<clip scene="4"/>
				<clip scene="5"/>
			</group>
			<group id="music" track="2" colour="blue">
				<clip scene="0"/>
				<clip scene="1"/>
				<clip scene="3"/>
				<clip scene="4"/>
				<clip scene="5"/>
			</group>
		</song>

		<song name="Bumpy Bosbo" colour="yellow" tempo="133" sceneoffset="0" pic="songs/cat.png" visuals="bumpybosbo">
			<notes>
				<text>This one goes around G and D
					Play bendy synth.</text>
			</notes>
			<controls>
				<keyboard>
					<range channel="2" transpose="24" low="0" high="62">BASS</range>
					<range channel="3" transpose="-12" low="63" high="120">STRINGS</range>
				</keyboard>
				<fader id="0" name="strings" channel="3" cc="71">
					<lcd text="strings"/>
				</fader>
				<fader id="2" name="bassfreq" channel="2" cc="71">
					<lcd text="bassfreq"/>
				</fader>
				<joystick camera="true">
					<pitchbend channel="2"/>
					<up name="mod wheel" channel="2" cc="1"/>
				</joystick>
			</controls>
			<!-- should validate for repetition -->
			<group id="beats" colour="red" track="0">
				<clip pad="0" scene="0">
					<visual scene="0" />
				</clip>
				<clip pad="1" scene="1">
					<visual scene="1" />
				</clip>
				<clip pad="4" scene="2">
					<visual scene="2" />
				</clip>
				<clip pad="5" scene="3">
					<visual scene="3" />
				</clip>
				<clip pad="12" scene="4">
					<visual scene="4" />
				</clip>
			</group>
			<group id="rhythm" colour="yellow" track="2">
				<clip scene="0"/>
				<clip scene="1"/>
			</group>
			<group id="lead" colour="pink" track="3">
				<clip scene="0"/>
				<clip scene="1"/>
			</group>
			<group id="bass" colour="purple" track="1">
					<clip scene="0"/>
				<clip scene="1"/>
			</group>
			<group id="bits" colour="green" track="3">
				<clip scene="3"/>
			</group>
		</song>

		<song name="Song Of Technology" colour="green" sceneoffset="10" visuals="songoftechnology">
			<notes>
				<text>Lorem ipsum dolor sit amet.</text>
			</notes>
			<controls>
				<keyboard channel="16" low="48" high="59">
					stretch/phaser
					loop/filter
					xloop/crush
					stutter/chorus
					tonal/scratchloop
					vinyl</keyboard>
					<keyboard channel="5" low="60">
						Synth
					</keyboard>
				<fader id="0" name="noise" channel="5" cc="71"/>
				<fader id="1" name="cutoff" channel="5" cc="72"/>
				<fader id="2" name="rate" channel="5" cc="73"/>
				<fader id="3" name="SOUND" channel="5" cc="74"/>
				<joystick>
					<pitchbend channel="5"/>
					<up name="mod wheel" channel="5" cc="1"/>
					<down name="mod wheel" channel="5" cc="1"/>
				</joystick>
			</controls>
			<group id="vocals" colour="pink" track="3">
				<clip pad="10" scene="1" >
					<visual scene="1" />
				</clip>
				<clip pad="11" scene="2" >
					<visual scene="2" />
				</clip>
				<clip pad="15" scene="3" >
					<visual scene="3" />
				</clip>
			</group>
			<group id="beats" colour="red" track="0">
				<clip pad="0" scene="0"/>
				<clip pad="4" scene="1"/>
				<clip pad="12" scene="3">
					<visual scene="0"/>
				</clip>
				<clip pad="7" scene="5">
					<visual scene="0"/>
				</clip>
			</group>
			<group id="bass" colour="purple" track="1">
				<clip pad="1" scene="0"/>
				<clip pad="5" scene="1"/>
				<clip pad="9" scene="2"/>
				<clip pad="13" scene="3"/>
				<clip pad="2" scene="4"/>
				<clip pad="6" scene="5"/>
			</group>
			<group id="trumpet" colour="yellow" track="2">
				<clip pad="14" scene="3">
					<visual scene="0"/>
				</clip>
			</group>


		</song>
		<song name="Violet Sky" sceneoffset="18" colour="purple" tempo="75" visuals="forest">
			<notes>
				<image source="songs/violet_sky_score.png"/>
				<text/>
			</notes>
			<controls>
				<keyboard channel="3">piano</keyboard>
				<fader id="0" name="strings"  channel="3" cc="71">
					<lcd text="strings"/>
				</fader>
				<multilayerfader id="0">
					<range from="0" to="0.25" layer="strings1"/>
					<range from="0.25" to="0.5" layer="strings2"/>
					<range from="0.5" to="0.75" layer="strings3"/>
					<range from="0.75" to="1" layer="strings4"/>
				</multilayerfader>				
			</controls>
			<group id="beats" colour="red" track="0">
				<clip pad="0" scene="0"/>
				<clip pad="4" scene="1"/>
				<clip pad="8" scene="2"/>
			</group>
			<group id="bass" colour="blue" track="1">
				<clip pad="1" scene="0"/>
				<clip pad="5" scene="1"/>
			</group>
			<group id="trumpets" colour="yellow" track="2">
				<clip pad="6" scene="1"/>
				<clip pad="10" scene="2"/>
			</group>
			<group id="backing" track="3" colour="purple">
				<clip scene="0"/>
				
			</group>
		</song>


		<song name="Shift" tempo="100" colour="" sceneoffset="0">
			<controls>
				<keyboard channel="16" low="48" high="59">
					stretch/phaser
					loop/filter
					xloop/crush
					stutter/chorus
					tonal/scratchloop
					vinyl</keyboard>
					<fader id="0" name="Beats" max="127.0" track="0"/>
					<fader id="1" name="Bass"  max="127.0" track="1"/>
					<fader id="2" name="Music" max="127.0" track="2"/>
					<fader id="3" name="Music" max="127.0" track="3"/>
			</controls>
			<notes>
				<text />
			</notes>
			
		</song>
	</songs>

	<common>
		<encoder id="7" name="tempo" channel="1" cc="2">
			<lcd id="7" text="TEMPO"/>
		</encoder>
		<button id="SETTING" name="reverb" channel="2" cc="107" type="gate"/>
		<button id="MESSAGE" name="delay" channel="2" cc="106" type="toggle"/>
		<button id="SCENE" name="beatrepeat" channel="1" cc="10" type="gate">
			<lcd id="5" text="GRID"/>
			<lcd id="6" text="PITCH"/>
		</button>
		<encoder id="5" name="beatrepeat" channel="1" cc="4"/>
		<encoder id="6" name="pitch" channel="1" cc="5"/>
		<button id="EXIT" name="effectrix" channel="16" note="C3">
			<lcd id="6" text="PATTERN"/>
		</button>
		<fader id="7" name="FILTER" channel="3" cc="5">
			<lcd id="7" text="FILTER"/>
		</fader>
	
	</common>

