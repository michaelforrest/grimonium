<?xml version="1.0" encoding="UTF-8"?>
<set>
	<songs>
		<!-- BUMPY BOSBO -->
		<song name="Bumpy Bosbo" colour="yellow" tempo="133" sceneoffset="0" pic="songs/cat.png">
			<stage zoom="0" pan="0">
				<layer id="bosbodrums"/>
				<layer id="basscat"/>
				<layer id="trancecat"/>
				<layer id="leadcat"/>
			</stage>
			<notes>
				<text>This one goes around G
					Play bendy synth.</text>
			</notes>
			<controls>
				<fader id="0" name="BEATS" max="110.0" track="0">
					<lcd id="0" text="BEATS" />
				</fader>
				<fader id="1" name="FILTER" channel="3" cc="5">
					<lcd id="1" text="FILTER"/>
				</fader>
				<encoder id="0" name="filter" channel="3" cc="5">
					<lcd text="FILTER"/>
				</encoder>
			</controls>
			<!-- should validate for repetition -->
			<group id="beats" colour="red" track="0">
				<freqbone low="kick" med="snare" high="hihat"/>
				<clip pad="0" scene="0"/>
				<clip pad="1" scene="1"/>
				<clip pad="4" scene="2"/>
				<clip pad="5" scene="3"/>
				<clip pad="12" scene="4"/>
			</group>
			<group id="rhythm" colour="yellow" track="2">
				<freqbone mid="bosbotrance"/>
				<clip pad="2" scene="0"/>
				<clip pad="6" scene="1"/>
			</group>
			<group id="lead" colour="pink" track="3">
				<freqbone high="bosbolead"/>
				<clip pad="3" scene="0"/>
				<clip pad="7" scene="1"/>
			</group>
			<group id="bass" colour="purple" track="1">
				<freqbone low="bosbobass"/>
				<clip pad="10" scene="0"/>
				<clip pad="11" scene="1"/>
			</group>
			<group id="bits" colour="green" track="3">
				<freqbone med="bosbobits" high="bosbohibits"/>
				<clip pad="13" scene="3"/>
			</group>
		</song>
		<!-- HOROSKOPE -->
		<song name="Horoskope" colour="red" tempo="105" sceneoffset="6">
			<stage pan="0" zoom="0">
				<layer id="gabbadrums"/>
				<layer id="horoskope"/>
			</stage>
			<notes>
				<image source="songs/violet_sky_score.png"/>
				<text>Horoskope</text>
			</notes>
			<group id="beats" colour="red" track="4">
				<notebone note="A#3" bone="kick"/>
				<notebone note="G2" bone="hat"/>
				<notebone note="D#2" bone="snare"/>
				<clip pad="0" scene="0"/>
			</group>
			<group id="bass" colour="purple" track="5">
				<clip pad="12" scene="0"/>
			</group>
			<group id="bits" colour="green" track="6">
				<clip pad="13" scene="0"/>
			</group>
			<group id="bits" colour="green" track="7">
				<clip pad="14" scene="0"/>
			</group>
			<group id="lead" colour="pink" track="8">
				<noterangebone low="48" high="72" bone="horoskope"/>
				<clip pad="3"  scene="0"/>
				<clip pad="7"  scene="1"/>
				<clip pad="11" scene="2"/>
				<clip pad="15" scene="3"/>
			</group>
		</song>
		<song name="Song Of Technology" colour="green" sceneoffset="10">
